{
   "background": {
      "persistent": false,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Twitch Auto Theater"
   },
   "content_scripts": [ {
      "js": [ "js/jquery.min.js", "js/inject.js" ],
      "matches": [ "http://www.twitch.tv/*", "https://www.twitch.tv/*" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Extension to automatically open Theater Mode on Twitch streams",
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Twitch Auto Theater",
   "options_page": "options.html",
   "permissions": [ "http://www.twitch.tv/*", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.8"
}
